        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/ssn/systems/hasSystemCapability"/>
                <owl:hasValue rdf:resource="http://esonetyellowpages.com/vocab/5c105"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
