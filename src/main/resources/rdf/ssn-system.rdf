  <rdf:Description rdf:about="http://www.w3.org/ns/ssn/systems/qualityOfObservation">
    <rdfs:label xml:lang="en">quality of observation</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/ssn/systems/qualityOfObservation">
